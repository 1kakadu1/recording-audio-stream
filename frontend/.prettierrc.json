{
	"bracketSpacing": true,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"useTabs": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"/@types",
		".model",
		"/const",
		".schema",
		".context",
		".fetch",
		"@/components/",
		".component",
		"@/hooks/",
		"ui/icons",
		"services/",
		"utils",
		"assets/",
		"^[./]",
		"(.scss|.css)"
	],
	"singleAttributePerLine": true,
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true
}
