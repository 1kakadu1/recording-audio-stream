{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:fix": "prettier --write . && eslint . --fix",
    "format:check": "npx prettier --list-different --ignore-path=.gitignore .prettierignore \"**/*.ts(x)?\"",
    "format:fix": "npx prettier --list-different --ignore-path=.gitignore .prettierignore \"**/*.ts(x)?\" --write",
    "pre-commit": "npm run format:fix && npm run format:check && npm run build",
    "nc": "cd .hygen && hygen new component",
    "nci": "cd .hygen && hygen new icon",
    "nctx": "cd .hygen && hygen new context"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.3.2",
    "@cspell/eslint-plugin": "^9.4.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "prettier": "^3.7.4",
    "react-tooltip": "^5.30.0",
    "sass": "^1.94.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
